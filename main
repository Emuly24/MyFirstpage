        function calculateAbandnmentScore() {
            abandonmentScore = 0; // Reset the total score
            let hasHighRating = false;

            // Loop through all questions to calculate the total score
            for (let i = 1; i <= 10; i++) {
                const radios = document.getElementsByName(`q${i}`);
                let value = 0;
                for (const radio of radios) {
                    if (radio.checked) {
                        value = parseInt(radio.value);
                        break;
                    }
                }
                if (!isNaN(value)) {
                    abandonmentScore += value;
                }
            }

            // Display total score automatically
            displayResults();
        }

        function displayResults() {
            respondentName = document.getElementById("name").value; // Get respondent's name
            let resultText = `Hi ${respondentName}!\nYour Abandonment Core belief  score is: ${abandnmentScore}\n\n`;
            if (abandnmentScore >= 10 && abandnmentScore <= 19) {
                resultText += "Very low.\nThis core belief probably does not apply to you.";
            } else if (abandnmentScore >= 20 && abandnmentScore <= 29) {
                resultText += "Fairly low.\nThis core belief may apply only occasionally.";
            } else if (abandnmentScore >= 30 && abandnmentScore <= 39) {
                resultText += "Moderate.\nThis core belief is an issue in your life.";
            } else if (abandnmentScore >= 40 && abandnmentScore <= 49) {
                resultText += "High.\nThis is definitely an important core belief for you.";
            } else if (abandnmentScore >= 50 && abandnmentScore <= 60) {
                resultText += "Very high.\nThis is a powerful core belief for you.";
            } else {
                resultText += "Please ensure all questions are answered correctly.";
            }

            // Display result in the HTML
            document.getElementById("result").innerText = resultText;
        }
        function calculateMistrustandAbusetScore() {
            mistrustandAbuseScore = 0; // Reset the total score
            let hasHighRating = false;

            // Loop through all questions to calculate the total score
            for (let i = 1; i <= 10; i++) {
                const radios = document.getElementsByName(`q${i}`);
                let value = 0;
                for (const radio of radios) {
                    if (radio.checked) {
                        value = parseInt(radio.value);
                        break;
                    }
                }
                if (!isNaN(value)) {
                   mistrustandAbuseScore += value;
                }
            }

            // Display total score automatically
            displayResults();
        }

        function displayResults() {
            respondentName = document.getElementById("name").value; // Get respondent's name
            let resultText = `Hi ${respondentName}!\nYour Mistrust and Abuse Core blief score is: ${abandnmentScore}\n\n`;
            if (mistrustandAbuseScore >= 10 && mistrustandAbuseScore <= 19) {
                resultText += "Very low.\nThis core belief probably does not apply to you.";
            } else if (mistrustandAbuseScore >= 20 && mistrustandAbuseScore <= 29) {
                resultText += "Fairly low.\nThis core belief may apply only occasionally.";
            } else if (mistrustandAbuseScore >= 30 && mistrustandAbuseScore <= 39) {
                resultText += "Moderate.\nThis core belief is an issue in your life.";
            } else if (mistrustandAbuseScore >= 40 && mistrustandAbuseScore <= 49) {
                resultText += "High.\nThis is definitely an important core belief for you.";
            } else if (mistrustandAbuseScore >= 50 && mistrustandAbuseScore <= 60) {
                resultText += "Very high.\nThis is a powerful core belief for you.";
            } else {
                resultText += "Please ensure all questions are answered correctly.";
            }

            // Additional notes for high individual ratings
            if (mistrustandAbuseScore < 30 && hasHighRating) {
                resultText += `\n\nNote: Although your total score is low, you rated at least one statement as a 5 or 6. This indicates that this core belief is still an issue in your life.\n\nConsider the following possibilities:\n`;
                resultText += "- You may have felt like your parents kept information from you.\n";
                resultText += "- There may have been secret-keeping among family members.\n";
                resultText += "- There was a lack of open communication in your family.\n";
                resultText += "- There was an intangible climate of distrust.\n";
                resultText += "- You were criticized or ridiculed when you were most vulnerable.\n";
                resultText += "- You were bullied, ridiculed, or humiliated by your peers.";
            }

            // Display result in the HTML
            document.getElementById("result").innerText = resultText;
        }
        function calculateEmotionalDeprivationtScore() {
