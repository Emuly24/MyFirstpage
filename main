        }
function displayResults() {
    respondentName = document.getElementById("name").value; // Get respondent's name
    let resultText = `Hi ${respondentName}!\nYour Mistrust and Abuse Core blief score is: ${abandnmentScore}\n\n`;
    if (emotionaldeprivationScore >= 10 && emotionaldeprivationScore <= 19) {
        resultText += "Very low.\nThis core belief probably does not apply to you.";
    } else if (emotionaldeprivationScore >= 20 && emotionaldeprivationScore <= 29) {
        resultText += "Fairly low.\nThis core belief may apply only occasionally.";
    } else if (emotionaldeprivationScore >= 30 && emotionaldeprivationScore <= 39) {
        resultText += "Moderate.\nThis core belief is an issue in your life.";
    } else if (emotionaldeprivationScore >= 40 && emotionaldeprivationScore <= 49) {
        resultText += "High.\nThis is definitely an important core belief for you.";
    } else if (emotionaldeprivationScore >= 50 && emotionaldeprivationScore <= 60) {
        resultText += "Very high.\nThis is a powerful core belief for you.";
    } else {
        resultText += "Please ensure all questions are answered correctly.";
    }

    // Additional notes for high individual ratings
    if (emotionaldeprivationScore< 30 && hasHighRating) {
        resultText += `\n\nNote:\n If you feel like this core belief is significant to your life though your score is lower than you expected, then you might want to consider some of the following childhood situations that could have contributed to your feelings:\n\\n`;
        resultText += "- You felt less loved than a sibling or siblings.\n";
        resultText += "- Your parents were such a tight unit that you felt left out.\n";
        resultText += "- You felt different than the rest of your family (“I always wondered if I was adopted”) so you felt less understood or loved.\n";
        resultText += "- You might have felt different from your peers and lacked the friend connections that happen in childhood and adolescence.\n";
        resultText += "- You were made to feel like the needs and feelings of others were more important than yours.\n";
        resultText += "- Your feelings or experiences weren't valued or validated.";
    }

    // Display result in the HTML
    document.getElementById("result").innerText = resultText;

}
function calculateDefectivenessScore() {
            defectivenessScore = 0; // Reset the total score
            let hasHighRating = false;

            // Loop through all questions to calculate the total score
            for (let i = 1; i <= 10; i++) {
                const radios = document.getElementsByName(`q${i}`);
                let value = 0;
                for (const radio of radios) {
                    if (radio.checked) {
                        value = parseInt(radio.value);
                        break;
                    }
                }
                if (!isNaN(value)) {
                   defectivenessScore += value;
                }
            }

            // Display total score automatically
            displayResults();
        }
        
function displayResults() {
    respondentName = document.getElementById("name").value; // Get respondent's name
    let resultText = `Hi ${respondentName}!\nYour Mistrust and Abuse Core blief score is: ${abandnmentScore}\n\n`;
    if (defectivenessScore >= 10 && defectivenessScore <= 19) {
        resultText += "Very low.\nThis core belief probably does not apply to you.";
    } else if (defectivenessScore >= 20 && defectivenessScore <= 29) {
        resultText += "Fairly low.\nThis core belief may apply only occasionally.";
    } else if (defectivenessScore >= 30 && defectivenessScore <= 39) {
        resultText += "Moderate.\nThis core belief is an issue in your life.";
    } else if (defectivenessScore >= 40 && defectivenessScore <= 49) {
        resultText += "High.\nThis is definitely an important core belief for you.";
    } else if (defectivenessScore >= 50 && defectivenessScore <= 60) {
        resultText += "Very high.\nThis is a powerful core belief for you.";
    } else {
        resultText += "Please ensure all questions are answered correctly.";
    }

    // Additional notes for high individual ratings
    if (defectivenessScore < 30 && hasHighRating) {
        resultText += `\n\nNote:\n This core belief is significant for many people and it can be a perceived internal or external feeling of defectiveness. You may have scored low on this questionnaire but you know that the feeling of defectiveness is with you.\nHere are some additional situations that may resonate with you:\n\\n`;
        resultText += "- There was a physical characteristic that caused embarrassment or was the target of ridicule by others.\n";
        resultText += "- There was something that made you feel self-conscious and you feared that others would discover it.\n";
        resultText += "- You felt that there was something wrong with you because of the way that you were treated by a member or members of your family or peers.\n";
        resultText += "- ou may have struggled with your gender identity or sexual orientation.\n";
        resultText += "- You felt less than because you are an adopted child in a family with biological children or because you are a different race or ethnicity than your adoptive parents.\n";
        resultText += "- You could never shake the feeling that something must be wrong with you if your biological mother put you up for adoption";
        resultText += "- You have had an interest that varied from the mainstream and it made you feel different.\n";
        resultText += "- You were afflicted with a childhood illness or disorder that left you feeling like something was wrong with you.\n";
    }

    // Display result in the HTML
    document.getElementById("result").innerText = resultText;
}
function calculateFailureScore() {
            failureScore = 0; // Reset the total score
            let hasHighRating = false;

            // Loop through all questions to calculate the total score
            for (let i = 1; i <= 10; i++) {
                const radios = document.getElementsByName(`q${i}`);
                let value = 0;
                for (const radio of radios) {
                    if (radio.checked) {
                        value = parseInt(radio.value);
                        break;
                    }
                }
                if (!isNaN(value)) {
                   failureScore += value;
                }
            }

            // Display total score automatically
            displayResults();
        }

        function displayResults() {
            respondentName = document.getElementById("name").value; // Get respondent's name
            let resultText = `Hi ${respondentName}!\nYour Mistrust and Abuse Core blief score is: ${abandnmentScore}\n\n`;
            if (failureScore >= 10 && failureScore <= 19) {
                resultText += "Very low.\nThis core belief probably does not apply to you.";
            } else if (failureScore >= 20 && failureScore <= 29) {
                resultText += "Fairly low.\nThis core belief may apply only occasionally.";
            } else if (failureScore >= 30 && failureScore <= 39) {
                resultText += "Moderate.\nThis core belief is an issue in your life.";
            } else if (failureScore >= 40 && failureScore <= 49) {
                resultText += "High.\nThis is definitely an important core belief for you.";
            } else if (failureScore >= 50 && failureScore <= 60) {
                resultText += "Very high.\nThis is a powerful core belief for you.";
            } else {
                resultText += "Please ensure all questions are answered correctly.";
            }

            // Additional notes for high individual ratings
            if (failureScore < 30 && hasHighRating) {
                resultText += `\n\nNote:\n If you have a low score but you feel like you're a failure, then consider these additional experiences that may have contributed to this core belief::\n`;
                resultText += "- Your parents were successful, wealthy, accomplished, talented, or well known and you felt like a failure by comparison.\n";
                resultText += "- Your parents set unrealistic expectations for you (e.g., you were told from the time you were in kindergarten that you would go to Harvard).\n";
                resultText += "- Your sibling(s) was more attractive, talented, and successful.\n";
                resultText += `\n\nThese could have been true or they could have been your perception. Either way it is what you believed and that is what contributed to this core belief\n`;
                
            }

            // Display result in the HTML
            document.getElementById("result").innerText = resultText;
        }

