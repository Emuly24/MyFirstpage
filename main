    <script>
        let respondentName = ""; // To store respondent's name
        let abandonmentScore = 0; // To store the total score globally
        let mistrustandAbuseScore = 0; // To store the total score globally
        let emotionaldeprivationScore = 0; // To store the total score globally
        let defectivenessScore = 0; // To store the total score globally
        let failureScore = 0; // To store the total score globally
        let hasHighRating = false; // To track if any question was rated 5 or 6

        function checkNameAndShowAssessment() {
            const nameInput = document.getElementById("name").value.trim(); // Get and trim the name
            const messageElement = document.getElementById("name-message"); // Reference the message display element
            const assessmentElement = document.getElementById("assessment"); // Reference the assessment section
            const nameInputSection = document.getElementById("name-section"); // Reference the name input section

            if (nameInput) {
                // Show welcoming message
                if (nameInput.toLowerCase() === "maggie zgambo") {
                    messageElement.innerText = "Thank you Maa, my dear for your choice in taking this assessment. Please be honest with yourself. It is only you who will be able to see your own results. Be flexible; transcend the past.";
                } else {
                    messageElement.innerText = `Thank you dear ${nameInput},  for your choice in taking this assessment. Please be honest with yourself. It is only you who will be able to see your own results. Be flexible; outlive the past.`;
                }
                // Hide name input and show the assessment
                nameInputSection.style.display = "none"; // Hides the input field and label
                assessmentElement.style.display = "block"; // Shows the assessment
            } else {
                // Clear the message and keep the assessment hidden if no name is entered
                messageElement.innerText = "";
                assessmentElement.style.display = "none";
            }
        }
    </script>
